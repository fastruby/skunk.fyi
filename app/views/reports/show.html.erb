<div class="report">
  <table class="results">
    <thead>
      <tr>
        <% AnalyzedModule::KEYS.each do |value| %>
          <th><%= value.humanize %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
    <% @report.data.each do |part| %>
      <tr class="<%= cycle("odd", "even") %>">
        <% AnalyzedModule::KEYS.each do |value| %>
          <td class="<%= cell_class(value) %>">
            <%= part[value] %>
          </td>
        <% end %>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
